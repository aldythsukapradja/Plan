<script>
  // Header drawer
  const hamb = document.getElementById('hamb');
  const drawer = document.getElementById('drawer');
  hamb.addEventListener('click', () => {
    hamb.classList.toggle('active');
    const open = hamb.classList.contains('active');
    drawer.classList.toggle('open', open);
    drawer.setAttribute('aria-hidden', !open);
    document.body.style.overflow = open ? 'hidden' : '';
  });
  drawer.querySelectorAll('.jump').forEach(a => a.addEventListener('click', () => {
    hamb.classList.remove('active'); drawer.classList.remove('open');
    drawer.setAttribute('aria-hidden','true'); document.body.style.overflow='';
  }));

  // Up button visibility
  const up = document.getElementById('up');
  const onScroll = () => {
    (window.scrollY > 320) ? up.classList.add('show') : up.classList.remove('show');
  };
  document.addEventListener('scroll', onScroll, {passive:true}); onScroll();

  // Compact table toggle
  const toggle = document.getElementById('toggleTable');
  const table = document.getElementById('timelineTable');
  toggle.addEventListener('click', ()=>{
    const show = table.style.display !== 'block';
    table.style.display = show ? 'block' : 'none';
    toggle.setAttribute('aria-expanded', show ? 'true' : 'false');
    if (show) table.scrollIntoView({behavior:'smooth', block:'start'});
  });

  // Accordions
  document.querySelectorAll('.acc-btn').forEach(btn=>{
    btn.addEventListener('click', ()=>{
      const panel = btn.nextElementSibling;
      const open = panel.style.display === 'block';
      // single-open behavior
      document.querySelectorAll('.acc-panel').forEach(p=>p.style.display='none');
      document.querySelectorAll('.acc-btn').forEach(b=>{b.setAttribute('aria-expanded','false'); b.lastElementChild.textContent='▸';});
      if(!open){ panel.style.display='block'; btn.setAttribute('aria-expanded','true'); btn.lastElementChild.textContent='▾'; }
    });
  });

  // Footer info
  document.getElementById('yr').textContent = new Date().getFullYear();
  document.getElementById('build').textContent = new Date().toLocaleString();

  // Prevent GO placeholder navigation (optional)
  document.querySelectorAll('a.go[href="#"]').forEach(a=>{
    a.addEventListener('click', e=>{
      e.preventDefault();
      const name = a.getAttribute('data-link') || 'placeholder';
      alert('GO placeholder:\n' + name + '\n\nReplace href with the real link when ready.');
    });
  });
</script>
</body>
</html>
